{
  "name": "web-driverify",
  "version": "1.0.0",
  "description": "A passive web driver endpoint for real browsers.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node bin/www",
    "debug": "NODE_ENV=development DEBUG=wd:* babel-node bin/www",
    "build": "babel src -d lib/src && babel test/fixtures -d lib/test/fixtures && babel bin -d lib/bin",
    "test:unit": "NODE_ENV=development DEBUG=wd:* mocha test/unit/*.js --compilers js:babel-core/register",
    "test:prepare": "NODE_ENV=development DEBUG=wd:* babel-node bin/wd-with-phantom.js",
    "test:integration": "NODE_ENV=development DEBUG=wd:* ./node_modules/.bin/wdio wdio.conf.js",
    "test": "npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-driverify/web-driverify.git"
  },
  "keywords": [
    "proxy",
    "web-driver",
    "mobile"
  ],
  "author": "harttle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/web-driverify/web-driverify/issues"
  },
  "homepage": "https://github.com/web-driverify/web-driverify#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "charset": "^1.0.0",
    "connect-timeout": "^1.8.0",
    "debug": "^2.6.1",
    "express": "^4.14.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.0",
    "morgan-debug": "^2.0.0",
    "request": "^2.79.0",
    "shopify-liquid": "^1.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "node-horseman": "^3.2.0",
    "superagent": "^3.4.1",
    "superagent-proxy": "^1.0.2",
    "supertest": "^3.0.0",
    "wdio-mocha-framework": "^0.5.8",
    "wdio-spec-reporter": "0.0.5",
    "webdriverio": "^4.6.2"
  }
}
